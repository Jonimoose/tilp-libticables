#: src/detect.c:71 src/type2str.c:147 src/win32/win32_detect.c:59
msgid "unknown"
msgstr "inconnu"

#: src/error.c:54
msgid "Msg: Unable to open serial device."
msgstr "Msg : Impossible d'ouvrir le périphérique série."

#: src/error.c:56
msgid ""
"Cause: check your permissions on the node. Check that the device is not "
"locked by another application (modem ?)."
msgstr ""
"Cause : Vérifiez que vous avez les permissions nécessaires sur ce noeud. "
"Vérifiez que le périphérique n'est pas bloqué par une autre application "
"(modem ?)."

#: src/error.c:60
msgid "Msg: Unable to open COM port."
msgstr "Msg : Impossible d'ouvrir le port COM."

#: src/error.c:62 src/error.c:94
msgid ""
"Cause: Check that the device is not used/locked by another application "
"(modem ?)."
msgstr ""
"Cause : Vérifiez que le périphérique n'est pas utilisé/bloqué par une autre "
"application (modem ?)."

#: src/error.c:66
msgid "Msg: Error occured while writing to the device."
msgstr "Msg : Erreur en écrivant sur le périphérique."

#: src/error.c:70
msgid "Msg: Timeout occured while writing to the device."
msgstr "Msg : Limite de temps dépassée en écrivant sur le périphérique."

#: src/error.c:72 src/error.c:82
msgid ""
"Cause: Check that your link cable is plugged and/or the calculator is ready."
msgstr ""
"Cause : Vérifiez que le câble est connecté et/ou que la calculatrice est "
"prête."

#: src/error.c:76
msgid "Msg: Error occured while reading to the device."
msgstr "Msg : Erreur de lecture du périphérique."

#: src/error.c:80
msgid "Msg: Timeout occured while reading to the device."
msgstr "Msg : Limite de temps dépassée en lisant le périphérique."

#: src/error.c:86
msgid "Msg: A uint8_t have been lost."
msgstr "Msg : Une donnée a été perdue."

#: src/error.c:88
msgid "Cause: Application too slow."
msgstr "Cause : Application trop lente."

#: src/error.c:92
msgid "Msg: CreateFile error."
msgstr "Msg : Fichier d'erreur créé."

#: src/error.c:98
msgid "Msg: Unable to open a node in /dev."
msgstr "Msg : Impossible d'ouvrir un noeud dans /dev."

#: src/error.c:100
msgid ""
"Cause:Check that the node exists. Check your devfs. Check that you have "
"installed the driver."
msgstr ""
"Cause : Vérifiez que le noeud existe et que vous avez installé le pilote "
"approprié. Vérifiez également le fichier devfs."

#: src/error.c:105 src/error.c:109
msgid "Msg: Unable to use parallel/serial port: access refused."
msgstr "Msg : Impossible d'utiliser le port parallèle/série : accès refusé."

#: src/error.c:107
msgid ""
"Cause: Check that you have needed permissions (super user privileges). Else, "
"you will need to use a kernel module (tipar/tiser)."
msgstr ""
"Cause : Vérifiez que vous avez les permissions nécessaires (privilèges super "
"utilisateur). Sinon, vous devrez utiliser un module du noyau (tipar/tiser)."

#: src/error.c:111
msgid ""
"Cause: Is PortTalk installed/started ? You may have to install TiLP with "
"administrator privileges."
msgstr ""
"Cause : Est-ce que PortTalk est installé/démarré ? Vous devrez peut-être "
"installer TiLP avec les privilèges d'administrateur."

#: src/error.c:116
msgid "Msg: No link cable has been found on the scanned port."
msgstr "Msg : Aucun câble n'a été trouvé sur le port scanné."

#: src/error.c:118 src/error.c:243
msgid "Cause: ??"
msgstr "Cause : ??"

#: src/error.c:122
msgid "Msg: Unable to open pipes for virtual linking."
msgstr "Msg : Impossible d'ouvrir les tubes pour la liaison virtuelle."

#: src/error.c:124
msgid ""
"Cause: Check that you have permissions to create a pipe in the /tmp "
"directory."
msgstr ""
"Cause : Vérifiez que vous avez les permissions de créer un tube (pipe) dans "
"le dossier /tmp."

#: src/error.c:128
msgid "Msg: Unable to close pipes."
msgstr "Msg : Impossible de fermer les tubes (pipes)."

#: src/error.c:130
msgid "Cause: System error ?!"
msgstr "Cause : Erreur du système ?"

#: src/error.c:134
msgid "Msg: CreateFileMapping error."
msgstr "Msg : Fichier MappingError créé."

#: src/error.c:136
msgid ""
"Cause: Trying to communicate without correspondent. Did you launch the "
"emulator before running TiLP ?"
msgstr ""
"Cause : Communication sans correspondant. Avez-vous lancé l'émulateur avant "
"de démarrer TiLP ?"

#: src/error.c:140
msgid "Msg: IOCTL error."
msgstr "Msg : Erreur IOCTL"

#: src/error.c:144
msgid ""
"Cause: Check that you have required rights on the node and/or your kernel "
"module is loaded."
msgstr ""
"Cause : Vérifiez que vous avez les permissions nécessaires sur le nœud et/ou "
"que le module du noyau est chargé."

#: src/error.c:148
msgid "Msg: SetupComm error."
msgstr "Msg : Erreur à l'établissement de la communication."

#: src/error.c:150 src/error.c:156 src/error.c:162 src/error.c:168
#: src/error.c:174
msgid "Cause: Internal Win32 error."
msgstr "Cause : Erreur interne à Win32."

#: src/error.c:154
msgid "Msg: SetCommState error."
msgstr "Msg : Erreur SetCommState."

#: src/error.c:160
msgid "Msg: GetCommState error."
msgstr "Msg : Erreur GetCommState."

#: src/error.c:166
msgid "Msg: GetCommTimeouts error."
msgstr "Msg : Erreur GetCommTimeouts."

#: src/error.c:172
msgid "Msg: SetCommTimeouts error."
msgstr "Msg : Erreur SetCommTimeouts."

#: src/error.c:178
msgid "Msg: Error while flushing the buffer(s)."
msgstr "Msg : Erreur en libérant le(s) buffer(s)."

#: src/error.c:180
msgid "Cause: Internal Win32 error"
msgstr "Cause : Erreur interne à Win32."

#: src/error.c:184
msgid "Msg: IOCTL code unsuccessful."
msgstr "Msg : Échec du code IOTCL."

#: src/error.c:186
msgid "Cause: Internal device driver error."
msgstr "Cause : Erreur interne au pilote du périphérique."

#: src/error.c:191 src/error.c:197 src/error.c:203
msgid "Msg: Unable to open the USB device."
msgstr "Msg : Impossible d'ouvrir le périphérique USB."

#: src/error.c:195
msgid ""
"Cause: Check that you have required rights on the node and/or your driver is "
"loaded."
msgstr ""
"Cause : Vérifiez que vous avez les permissions nécessaires sur le nœud et/ou "
"que le pilote est chargé."

#: src/error.c:201
msgid ""
"Cause: Check your SilverLink is correcly installed in the Windows' Control "
"Panel. The ticables library need a specific driver for this cable. You will "
"find it on http://lpg.ticalc.org/prj_usb."
msgstr ""
"Cause : Vérifiez que votre SilverLink est correctement installé dans le "
"panneau de configuration Windows. La librairie ticables a besoin d'un pilote "
"spécifique pour ce câble. Vous le trouverez sur http://lpg.ticalc.org/"
"prj_usb."

#: src/error.c:205
msgid "Cause: Your cable is not connected or stalled."
msgstr "Cause : Votre câble est figé ou n'est pas connecté."

#: src/error.c:212
msgid "Msg: Error occurred while initializing the libusb."
msgstr "Msg : Erreur à l'initialisation de libusb."

#: src/error.c:216
msgid ""
"Cause: Check that your cable is connected or not stalled. Check your libusb "
"and usbfs, too."
msgstr ""
"Cause : Vérifiez que votre câble est connecté et qu'il n'est pas figé. "
"Vérifiez également libusb et usbfs."

#: src/error.c:218
msgid "Msg: Unable to initialize the USB device."
msgstr "Msg : Impossible d'initialiser le périphérique USB."

#: src/error.c:220
msgid "Cause: Your cable is probably not connected."
msgstr "Cause : Votre câble est probablement déconnecté."

#: src/error.c:225
msgid "Msg: Unable to open/find a USB device."
msgstr "Msg : Impossible d'ouvrir/trouver un périphérique USB."

#: src/error.c:229
msgid ""
"Cause: Check that your cable is connected or not stalled. Check you rlibusb "
"and usbfs, too."
msgstr ""
"Cause : Vérifiez que votre câble est connecté et qu'il n'est pas figé. "
"Vérifiez également libusb et usbfs."

#: src/error.c:233
msgid "Msg: Error while reseting USB endpoints."
msgstr "Msg : Erreur à la ré-initialisation des endpoints USB."

#: src/error.c:236
msgid "Cause: Your cable may be stalled. Unplug and replug it !."
msgstr ""
"Cause : Votre câble est peut-être figé/bloqué. Déconnectez et reconnectez-le."

#: src/error.c:241
msgid "Msg: FreeLibrary error. Unable to release the DLL."
msgstr "Msg : Erreur FreeLibrary. Impossible de libérer la DLL."

#: src/error.c:249
msgid "Msg: Unable to get a unique IPC (Inter Process Communication) key."
msgstr "Msg : Impossible d'obtenir une CIP (Clé Inter Processus) unique."

#: src/error.c:253
msgid ""
"Cause: Check that you have enough resources for allocating a shared memory "
"segment."
msgstr ""
"Cause : Vérifiez que vous disposez de suffisamment de mémoire pour allouer "
"un segment de mémoire partagée."

#: src/error.c:257
msgid "Msg: Unable to open a shared memory segment."
msgstr "Msg : Impossible d'ouvrir un segment de mémoire partagée."

#: src/error.c:259
msgid "Cause: Do you have any resources ?"
msgstr "Cause : Disposez-vous de suffisamment de ressources ?"

#: src/error.c:263
msgid "Msg: Unable to attach shared memory segment."
msgstr "Msg : Impossible de lier un segment de mémoire partagée."

#: src/error.c:265
msgid "Cause: Too many attachements ?"
msgstr "Cause : Trop de liens ?"

#: src/error.c:270
msgid "Msg: Unable to detach the shared memory segment."
msgstr "Msg : Impossible de libérer un segment de mémoire partagée."

#: src/error.c:272
msgid "Cause: Is segment locked ?"
msgstr "Cause : Le segment est-il vérrouillé ?"

#: src/error.c:277
msgid "Msg: Unable to destroy the shared memory segment."
msgstr "Msg : Impossible de supprimer le segment de mémoire partagée."

#: src/error.c:281
msgid "Cause: Check that no applications are still attached on it."
msgstr "Cause : Vérifiez qu'aucune application n'y est attachée."

#: src/error.c:285
msgid "Msg: Unable to open a file mapping."
msgstr "Msg : Impossible d'ouvrir un fichier d'allocation."

#: src/error.c:287
msgid "Cause: VTi seems to be not launched yet."
msgstr "Cause : VTi semble ne pas être encore lancé."

#: src/error.c:291
msgid "Msg: Illegal operation or argument."
msgstr "Msg : Opération ou argument illégal."

#: src/error.c:293
msgid "Cause: A bug in TiLP, mail to: roms@tilp.info."
msgstr "Cause : Ceci est un bogue de TiLP, signalez-le à : roms@tilp.info."

#: src/error.c:297
msgid "Msg: No I/O resource available !"
msgstr "Msg : Pas de ressource entrée/sortie disponible."

#: src/error.c:301
msgid ""
"Cause: This error may have some causes. See the previous error message for "
"more complete informations. Check for:\n"
"- I/O permissions (parallel/serial link cable)\n"
"- device driver (parallel/serial cable under Win NT4/2000/XP or USB)\n"
"- kernel module (parallel/serial or USB under Linux."
msgstr ""
"Cause : Cette erreur peut avoir plusieurs causes. Se référer aux messages "
"d'erreur précédents pour plus d'information. Vérifiez :\n"
"- les permissions d'entrée/sortie (câble parallèle/série)\n"
"- le pilote du périphérique (câble parallèle/série sous Win NT4/2000/XP ou "
"USB)\n"
"- le module du noyau (parallèle/série ou USB sous Linux)."

#: src/error.c:307
msgid ""
"Msg: Invalid port: try to use an I/O port device which is incompatible with "
"the link cable."
msgstr ""
"Msg : Port non valide : vous essayez d'utiliser un port d'entrée/sortie "
"incompatible avec le câble de liaison."

#: src/error.c:311
msgid ""
"Cause: It's due to a misuse of library, probably a TiLP bug. Mail to: "
"roms@tilp.info"
msgstr ""
"Cause : Ceci est dû à une mauvaise utilisation de la librairie, certainement "
"un bogue de TiLP. Signalez-le à: roms@tilp.info"

#: src/error.c:315
msgid "Msg: PortTalk driver and/or library not found."
msgstr "Msg : Pilote et/ou librairie PortTalk introuvable."

#: src/error.c:319
msgid ""
"Cause: TiLP should start this driver automagically through the SCM. Check "
"that it's loaded and TiLP as been installed with admin privileges !"
msgstr ""
"Cause : TiLP devrait lancer automatiquement ce pilote à travers SCM. "
"Vérifiez qu'il est chargé et que TiLP a bien été installé avec les "
"privilèges d'administrateur."

#: src/error.c:325
msgid "Msg: SilverLink library has an insufficient version number."
msgstr "Msg : La librairie SilverLink a un numéro de version insuffisant."

#: src/error.c:329
msgid ""
"Cause: the SilverLink driver currently installed has a wrong version. Please "
"upgrade !"
msgstr ""
"Cause : Une mauvaise version du pilote SilverLink a été installée. Veuillez "
"la mettre à jour."

#: src/error.c:333
msgid "Msg: the node does not exists."
msgstr "Msg : le nœud n'existe pas."

#: src/error.c:335
msgid "Cause: the node has not been created by the module."
msgstr "Cause : le nœud n'a pas été créé par le module."

#: src/error.c:339
msgid "Msg: you don't have permissions for reading/writing the node."
msgstr "Msg : vous n'avez pas les permissions de lecture/écriture sur le nœud."

#: src/error.c:341
msgid "Cause: others does not have r/w access or you are not in the group."
msgstr ""
"Cause : les utilisateurs normaux n'ont pas l'accès en lecture/écriture ou "
"vous n'appartenez pas à ce groupe."

#: src/error.c:345
msgid "Msg: module not loaded."
msgstr "Msg : le module n'est pas chargé."

#: src/error.c:347
msgid "Cause: the module does not exist or has not been loaded."
msgstr "Cause : le module n'existe pas ou n'a pas été préalablement chargé."

#: src/error.c:351
msgid "Msg: the usb pseudo-filesystem is not mounted."
msgstr "Msg : le système de fichier virtuel usb n'est pas monté."

#: src/error.c:353
msgid ""
"Cause: your kernel does not have usbfs support or it is not mounted by your /"
"etc/fstab."
msgstr ""
"Cause : votre noyau ne prend pas en charge usbfs, ou le système de fichier "
"n'a pas été monté par /etc/fstab."

#: src/error.c:357
msgid ""
"Error code not found in the list.\n"
"This is a bug. Please report it.\n"
"."
msgstr ""
"Code d'erreur inexistant dans la liste.\n"
"Ceci est un bogue. Veuillez le signaler.\n"
"."

